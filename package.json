{
  "name": "@badassery/laravel-up",
  "version": "0.3.0",
  "author": "Jesse Carter @WonderPanda",
  "private": false,
  "access": "public",
  "bin": {
    "lvl": "./bin/run"
  },
  "bugs": "https://github.com/badassery/laravel-up/issues",
  "dependencies": {
    "@oclif/command": "^1",
    "@oclif/config": "^1",
    "@oclif/plugin-help": "^2",
    "chalk": "^2.4.2",
    "clear": "^0.1.0",
    "cli-ux": "^5.3.2",
    "envfile": "^3.0.0",
    "execa": "^2.1.0",
    "figlet": "^1.2.4",
    "inquirer": "^7.0.0",
    "ip": "^1.1.5",
    "js-yaml": "^3.13.1",
    "listr": "^0.14.3",
    "lodash": "^4.17.15",
    "node-notifier": "^6.0.0",
    "rxjs": "^6.5.3",
    "shelljs": "^0.8.3",
    "tslib": "^1"
  },
  "devDependencies": {
    "@commitlint/cli": "^8.2.0",
    "@commitlint/config-conventional": "^8.2.0",
    "@commitlint/prompt": "^8.2.0",
    "@oclif/dev-cli": "^1",
    "@oclif/test": "^1",
    "@oclif/tslint": "^3",
    "@types/chai": "^4",
    "@types/figlet": "^1.2.0",
    "@types/inquirer": "^6.5.0",
    "@types/ip": "^1.1.0",
    "@types/js-yaml": "^3.12.1",
    "@types/listr": "^0.14.2",
    "@types/mocha": "^5",
    "@types/node": "^10",
    "@types/shelljs": "^0.8.5",
    "chai": "^4",
    "commitizen": "^4.0.3",
    "conventional-changelog-cli": "^2.0.23",
    "cz-conventional-changelog": "^3.0.2",
    "globby": "^10",
    "husky": "^3.0.5",
    "lint-staged": "^9.3.0",
    "mocha": "^5",
    "nyc": "^13",
    "oclif": "^1.14.0",
    "prettier": "^1.18.2",
    "ts-node": "^8",
    "tslint": "^5",
    "typescript": "^3.3"
  },
  "engines": {
    "node": ">=8.0.0"
  },
  "files": [
    "/bin",
    "/lib",
    "/environment",
    "/npm-shrinkwrap.json",
    "/oclif.manifest.json"
  ],
  "homepage": "https://github.com/badassery/laravel-up",
  "keywords": [
    "oclif",
    "laravel",
    "cli",
    "up"
  ],
  "license": "MIT",
  "main": "lib/index.js",
  "oclif": {
    "commands": "./lib/commands",
    "bin": "lvl",
    "plugins": [
      "@oclif/plugin-help"
    ]
  },
  "repository": "badassery/laravel-up",
  "scripts": {
    "commit": "yarn run git-cz",
    "postpack": "rm -f oclif.manifest.json",
    "posttest": "tslint -p test -t stylish",
    "prepack": "rm -rf lib && tsc -b && oclif-dev manifest && oclif-dev readme",
    "test": "nyc --extension .ts mocha --forbid-only \"test/**/*.test.ts\"",
    "version": "oclif-dev readme && git add README.md",
    "changelog": "yarn run conventional-changelog -i CHANGELOG.md -s -p angular",
    "gencommand": "yarn oclif command"
  },
  "config": {
    "commitizen": {
      "path": "./node_modules/cz-conventional-changelog"
    }
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged",
      "commit-msg": "commitlint -E HUSKY_GIT_PARAMS"
    }
  },
  "lint-staged": {
    "*.ts": [
      "yarn run prettier --write",
      "git add"
    ],
    "*.md": [
      "yarn run prettier --write",
      "git add"
    ]
  },
  "types": "lib/index.d.ts"
}
